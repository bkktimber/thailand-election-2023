{
	"compilerOptions": {
		"module": "NodeNext",
		"esModuleInterop": true,
		"target": "es6",
		"moduleResolution": "node",
		"sourceMap": true,
		"rootDir": "src",
		"outDir": "dist",
		"allowJs": true,
		// "lib": ["es2016"],
		"typeRoots": ["./node_modules/@types"]
	},
	"include": ["./src/**.*"],
	"exclude": ["node_modules", "dist"]
